{
  "extends": [
    "eslint:recommended",
    "standard"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "camelcase": "off",
    "comma-dangle": [
      "error",
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "never"
      }
    ],
    "max-len": "off",
    "no-console": "off",
    "no-unused-vars": "warn",
    "semi": [
      "error",
      "always"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 2
      }
    ],
    "quotes": [
      "error",
      "double"
    ]
  }
}
